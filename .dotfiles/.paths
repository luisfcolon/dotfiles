#!/usr/bin/env bash

# Paths and directories
export PATH=/usr/local/sbin:~/.composer/vendor/bin:/usr/local/bin:/usr/local/mysql/bin:$PATH
export PATH="$HOME/.rbenv/bin:$PATH"
eval "$(rbenv init -)"

# Remove duplicates from PATH
remove_dups() {
  local D=${2:-:} path= dir=
  while IFS= read -d$D dir; do
    [[ $path$D =~ .*$D$dir$D.* ]] || path+="$D$dir"
  done <<< "$1$D"
  printf %s "${path#$D}"
}

PATH=$(remove_dups "$PATH")

unset -f remove_dups
